[Unit]
Description=Gunicorn instance to serve mainpage
After=network.target

[Service]
User=yebin
Group=www-data
WorkingDirectory=/home/yebin/pyprojects/mainpage/
Environment="PATH=/home/yebin/pyprojects/mainpage/.venv/bin"
ExecStart=/home/yebin/pyprojects/mainpage/.venv/bin/gunicorn --workers 3 --bind unix:mainpage.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target
