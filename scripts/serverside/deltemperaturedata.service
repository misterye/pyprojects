[Unit]
Description=serve delete terminial temperature data service
After=network.target

[Service]
User=yebin
Group=root
WorkingDirectory=/home/yebin/pyprojects/monitor
Environment="PATH=/home/yebin/pyprojects/monitor/.venv/bin"
ExecStart=/home/yebin/pyprojects/monitor/.venv/bin/python /home/yebin/pyprojects/monitor/del_temperature_data.py

[Install]
WantedBy=multi-user.target
